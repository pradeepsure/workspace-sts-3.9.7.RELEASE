package org.pradeep;

public class ArmstrongExample
{
	public static void main(String[] args) 
	{
		int c=0,tmp,a;
		int n=153;
		tmp=n;
		while(n>0) 
		{
			a=n%10;
			n=n/10;
			c=c+(a*a*a);
			if(tmp==c)
				System.out.println("Given Number is Armstrong");
			else
				System.out.println("not an armstron");
		}
	}  
}
